<application>
<name>op_detector_example_module </name>

<dependencies>
</dependencies>

<module>
    <name> vPreProcess </name>
    <parameters>--flipx --flipy --split_stereo </parameters>
    <node> localhost </node>
</module>

<module>
    <name> op_detector_example_module </name>
    <parameters>--name /op_detector_example_module --models_path /openpose/models --pose_model BODY_25 </parameters>
    <node> localhost </node>
</module>

<module>
    <name> vFramerLite </name>
    <parameters> --displays "(/viewer (AE ISO))" </parameters>
    <node> localhost </node>
</module>

<module>
   <name> yarpview </name>
   <parameters>--name /view_out --synch </parameters>
   <node> localhost </node>
</module>


<connection>
  <from>/zynqGrabber/AE:o</from>
  <to>/vPreProcess/AE:i</to>
  <protocol> fast_tcp </protocol>
</connection>

<connection>
  <from>/vPreProcess/left:o</from>
  <to>/op_detector_example_module/AE:i</to>
  <protocol> fast_tcp </protocol>
</connection>

<connection>
  <from>/op_detector_example_module/AE:o</from>
  <to>/vFramer/viewer/AE:i</to>
  <protocol> fast_tcp </protocol>
</connection>

<connection>
  <from>/vFramer/viewer/image:o</from>
  <to>/view_out</to>
  <protocol>fast_tcp</protocol>
</connection>


</application>
